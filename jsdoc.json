{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": "src",
    "includePattern": "(.json|.js)$"
  },
  "plugins": [
    "node_modules/jsdoc/plugins/markdown"
  ],
  "opts": {
    "package": "./package.json",
    "readme": "./README.md",
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true,
    "private": true
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false
  }
}
